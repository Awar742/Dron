public class robota {
    public static void main(String args[]){
        cleaner rob=new cleaner();
        rob.showall();

        paymaster carl=new paymaster();
        carl.showall();
    }

    public static class cleaner {
        int sellery=150;
        String robota="cleaner";
        public void showall(){System.out.println(sellery+" "+robota);}
    }
    static class paymaster {
        int sellery=200;
        String robota="paymaster";
        public void showall(){System.out.println(sellery+" "+robota);}
    }

}
